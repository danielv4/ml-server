{
  "compilerOptions": {
      "baseUrl": ".",
      "target": "es6",
      "module": "es2015",
      "jsx": "react-jsx",
      "strict": false,
      "strictNullChecks": false,
      "noImplicitThis": true,
      "esModuleInterop": true,
